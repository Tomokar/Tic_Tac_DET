<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.TicTacDET" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="ncm_Theme" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- primary -->
        <item name="colorPrimary">@color/ncm_Orange</item>
        <item name="colorPrimaryVariant">@color/ncm_OrangeDark</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- secondary -->
        <item name="colorSecondary">@color/ncm_lightYellow</item>
        <item name="colorSecondaryVariant">@color/ncm_Yellow</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- status bar -->
        <item name="android:statusBarColor">@color/ncm_OrangeDark</item>
    </style>
</resources>